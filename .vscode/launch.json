{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Fox Algorithm",
            "type": "dart",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "env": {
                "ENVIRONMENT": "local"
            },
            "program": "${workspaceFolder}/bin/main.dart",
            "args": [
                "-a=fox_algorithm"
            ],
            "toolArgs": [],
            "console": "debugConsole"
        },
        {
            "name": "Fox FFI",
            "type": "dart",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "env": {
                "ENVIRONMENT": "local"
            },
            "program": "${workspaceFolder}/bin/main.dart",
            "args": [
                "-a=fox_ffi"
            ],
            "toolArgs": [],
            "console": "debugConsole",
            "preLaunchTask": "build:ffi:release"
        },
        {
            "name": "Benchmark",
            "type": "dart",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "env": {
                "ENVIRONMENT": "test"
            },
            "program": "${workspaceFolder}/bin/benchmark.dart",
            "args": [],
            "toolArgs": [],
            "console": "debugConsole",
            "runTestsOnDevice": false,
            "templateFor": "test",
            "preLaunchTask": "build:ffi:release"
        },
        {
            "name": "[test] All FBZ algorithms",
            "type": "dart",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "env": {
                "ENVIRONMENT": "test"
            },
            "program": "${workspaceFolder}/test/fbz_test.dart",
            "args": [],
            "toolArgs": [
                "--color",
                "--debug",
                "--coverage=.coverage",
                "--reporter=expanded",
                "--platform=vm", // chrome
                "--file-reporter=json:.coverage/tests.json",
                "--timeout=30s",
                "--concurrency=12"
            ],
            "console": "debugConsole",
            "runTestsOnDevice": false,
            "templateFor": "test",
            "preLaunchTask": "build:ffi:release"
        },
    ]
